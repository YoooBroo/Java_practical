 interface example {
    int add(int a,int b);
    int sub(int a,int b);
}

class Assingment implements example{
    public int add(int a,int b){
         return(a+b);
    }
    public int sub(int a,int b){
        return(a-b);
    }
}
public class Question_12{
    public static void main(String[] args) {
        example e1=new Assingment();

        System.out.println("Addition = "+e1.add(1,4));
        System.out.println("Subtraction = "+e1.sub(5,3));
    }
}
